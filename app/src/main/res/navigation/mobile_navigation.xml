<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.swapify.ui.home.HomeFragment"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_recived"
        android:name="com.example.swapify.ui.recived.RecivedFragment"
        tools:layout="@layout/fragment_recived" />

    <fragment
        android:id="@+id/navigation_send"
        android:name="com.example.swapify.ui.send.SendFragment"
        tools:layout="@layout/fragment_send" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.swapify.ui.settings.SettingsFragment"
        tools:layout="@layout/fragment_settings" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.example.swapify.ui.search.SearchFragment"
        tools:layout="@layout/fragment_search" />

</navigation>